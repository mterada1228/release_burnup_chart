# JIRA 接続設定（必須）
JIRA_URL=
JIRA_USERNAME=
JIRA_API_TOKEN=

# プロジェクト設定（必須）
JIRA_PROJECT_KEY=
JIRA_VERSION_NAME=

# Story Point フィールド設定（オプション）
# デフォルトは customfield_10016 ですが、環境によって異なる場合があります
JIRA_STORY_POINT_FIELD=customfield_10613

# ざっくりポイントフィールド設定（オプション）
# Story Point が設定されていない場合のフォールバック用
# スクリプト実行時のデバッグ情報で正しいフィールドIDを確認してください
JIRA_ZAKKURI_POINT_FIELD=customfield_10806

# 期間設定（オプション）
# 未設定の場合、開始日は90日前、終了日は今日が使用されます
# JIRA_START_DATE=2024-01-01
# JIRA_END_DATE=2024-12-31

# データポイント間の日数（オプション）
# デフォルトは7日（1週間）です
JIRA_INTERVAL_DAYS=7

# ベロシティ調整係数（オプション）
# 進捗予想の計算に使用する平均ベロシティを調整します
# 100 = 調整なし（デフォルト）
# 120 = 20%増（楽観的な予測）
# 80 = 20%減（保守的な予測）
# JIRA_VELOCITY_ADJUSTMENT=100
